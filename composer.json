{
	"name": "typro/bridge-nette-forms",
	"type": "library",
	"description": "Integration of Typro to Nette Forms.",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jan Pecha",
			"homepage": "https://www.janpecha.cz/"
		}
	],
	"funding": [
		{"type": "other", "url": "https://www.janpecha.cz/donate/"}
	],
	"require": {
		"php": "8.0 - 8.4",
		"nette/forms": "^2.4 || ^3.0"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"require-dev": {
		"nette/tester": "^2.0"
	},
	"extra": {
		"phpstan-extensions": [
			"phpstan/phpstan-nette"
		]
	},
	"suggest": {
		"typro/typro": "Typro installable via Composer."
	}
}
